{
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2016
  },
  "env": {
    "browser": true
  },
  "globals": {
    // constants
    "DATE_PATTERN": false,
    "NEWLINE_PATTERN": false,
    "ENABLE_EXTENSION_ID": false,
    "OFFSET_GMT_2": false,
    "CSS_CLASS_NOT_FOUND": false,
    "CSS_CLASS_FIND_ALL": false,
    "ELEM_ATTR_PREFIX": false,
    "BLACKLISTED_URLS": false,
    // functions
    "padNumberWithZero": false,
    "getDefaultTimezone": false,
    // classes
    "NumbersUtil": false,
    "DateObject": false,
    "StringUtil": false,
    "EndOfActionService": false,
    "MainTimeDisplayService": false,
    "StoreChangedDataService": false,
    "ChangeDatesService": false,
    "BlacklistService": false
  },
  "extends": "eslint:recommended",
  "rules": {
    "indent": [
      "error",
      4,
      {
        "SwitchCase": 1,
        "MemberExpression": 1
      }
    ],
    "quotes": [
      "error",
      "double"
    ],
    "no-console": "off",
    "semi": [
      "error",
      "always"
    ],
    "padded-blocks": "off",
    "space-before-function-paren": "off"
  }
}
